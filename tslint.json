{
  "extends": "tslint:recommended",
  "rules": {
     "quotemark": [true, "single", "avoid-escape", "jsx-double"],
     "trailing-comma": [false],
     "object-literal-sort-keys": false,
     "no-console": [false],
     "no-string-literal": false,
     "one-line": [false],
     "no-conditional-assignment": false,
     "switch-default": false
  }
}
